{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.sphinxjsc.spbase",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.7",
      "toolsId": "4ef2a30a-77fb-4ffa-9e50-09c337eb2c64"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sphinxjsc.spbase.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.sphinxjsc.spbase.view",
        "async": true,
        "viewPath": "com.sphinxjsc.spbase.view",
        "clearControlAggregation": false,
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouterGMS",
          "pattern": "ncc/{Status}/{UserName}",
          "target": [
            "TargetGMS"
          ]
        },
        {
          "name": "RouterDonHang",
          "pattern": "DonHang/{Magms}/{Mancc}",
          "target": [
            "TargetDonHang"
          ]
        },
        {
          "name": "RouterWorkflow",
          "pattern": "workflow/{WorkItemId}/{Status}/{UserName}",
          "target": [
            "TargetGMS"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "TargetGMS": {
          "viewId": "GMS",
          "viewName": "GMS",
          "transition": "slide",
          "clearControlAggregation": false,
          "title": "{i18n>appTitle}",
          "viewType": "XML"
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "title": "{i18n>appTitle}"
        },
        "TargetDonHang": {
          "viewId": "DonHang",
          "viewName": "DonHang",
          "transition": "slide",
          "clearControlAggregation": false,
          "title": "{i18n>appTitle}",
          "viewType": "XML"
        }
      }
    },
    "rootView": {
      "viewName": "com.sphinxjsc.spbase.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}