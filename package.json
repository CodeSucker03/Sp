{
  "name": "com.sphinxjsc.spbase",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@babel/eslint-parser": "7.14.7",
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "@sap-ux/ui5-middleware-fe-mockserver": "2",
    "@sap/ux-ui5-tooling": "1",
    "@sapui5/types": "~1.120.0",
    "@types/blob-stream": "^0.1.33",
    "@types/lodash.isfinite": "^3.3.9",
    "@types/lodash.isinteger": "^4.0.9",
    "@types/lodash.tonumber": "^4.0.9",
    "@types/pdfkit": "^0.13.9",
    "@types/svg-to-pdfkit": "^0.1.3",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.isstring": "^4.0.9",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@ui5/cli": "^4.0.16",
    "eslint": "8.57.0",
    "eslint-plugin-fiori-custom": "2.6.7",
    "typescript": "^5.1.6",
    "ui5-tooling-modules": "^3.30.6",
    "ui5-tooling-transpile": "^3.3.7"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#base-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#base-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "ts-typecheck": "tsc --noEmit",
    "prestart": "npm run ts-typecheck",
    "prebuild": "npm run ts-typecheck",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#base-display\"",
    "int-test": "fiori run --config ./ui5-mock.yaml --open 'test/integration/opaTests.qunit.html'",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#app-preview\"",
    "unit-test": "fiori run --config ./ui5-mock.yaml --open 'test/unit/unitTests.qunit.html'"
  },
  "sapuxLayer": "CUSTOMER_BASE",
  "dependencies": {
    
    "dayjs": "^1.11.19",
    "lodash.isequal": "^4.5.0",
    "lodash.isstring": "^4.0.1",
    "validator": "^13.15.23",
    "@types/numeral": "^2.0.5",
    "blob-stream": "^0.1.3",
    "css-unit-converter": "^1.1.2",
    "html-react-parser": "^5.2.2",
    "lodash.isfinite": "^3.3.2",
    "lodash.isinteger": "^4.0.4",
    "lodash.tonumber": "^4.0.3",
    "numeral": "^2.0.6",
    "pdfkit": "^0.16.0",
    "satori": "^0.12.1",
    "svg-to-pdfkit": "^0.1.8",
    "ui5-tooling-modules": "^3.22.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  }
}
